{
  "mcpServers": {
    "freshbooks": {
      "command": "python3",
      "args": [
        "/path/to/your/freshbooks_server.py"
      ],
      "env": {
        "FRESHBOOKS_CLIENT_ID": "your_client_id_here",
        "FRESHBOOKS_CLIENT_SECRET": "your_client_secret_here",
        "FRESHBOOKS_TUNNEL_URL": "https://your-tunnel-url.com",
        "FRESHBOOKS_LOCAL_PORT": "8000"
      },
      "disabled": false,
      "autoApprove": []
    }
  }
}
